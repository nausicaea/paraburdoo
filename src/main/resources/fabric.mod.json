{
	"schemaVersion": 1,
	"id": "paraburdoo",
	"version": "${version}",
	"name": "Paraburdoo",
	"description": "A small mod that adds additional recipes to PolyFactory",
	"authors": ["Eleanor Young <developer@nausicaea.net>"],
	"contact": {
		"homepage": "https://github.com/nausicaea/paraburdoo",
		"sources": "https://github.com/nausicaea/paraburdoo"
	},
	"license": "GPL-3.0-only",
	"icon": "assets/paraburdoo/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.nausicaea.paraburdoo.Paraburdoo"
		],
        "fabric-datagen": [
          "net.nausicaea.paraburdoo.ParaburdooDatagen"
        ]
	},
	"mixins": [
      "paraburdoo.mixins.json"
    ],
	"depends": {
		"fabricloader": "~${fabric_loader_version}",
		"minecraft": "~${minecraft_version}",
		"java": ">=21",
		"fabric-api": "*",
        "polyfactory": ">=${polyfactory_version}",
        "polymer": ">=${polymer_version}"

	},
    "suggests": {
        "polydex": ">=${polydex_version}"
    }
}
